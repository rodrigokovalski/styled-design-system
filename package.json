{
    "name": "@rodrigokovalski-ui/components",
    "version": "1.1.0",
    "description": "",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts --external react",
        "dev": "tsup src/index.ts --format esm,cjs --dts --external react --watch",
        "changeset": "changeset",
        "version-packages": "changeset version",
        "release": "npm run build && changeset publish",
        "lint": "eslint src/**/*.ts* --fix",
        "format": "prettier . --write",
        "prepare": "husky",
        "storybook": "storybook dev -p 6006",
        "build:storybook": "storybook build"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@changesets/cli": "^2.28.1",
        "@chromatic-com/storybook": "^3.2.6",
        "@eslint/js": "^9.23.0",
        "@storybook/addon-essentials": "^8.6.10",
        "@storybook/addon-interactions": "^8.6.10",
        "@storybook/addon-onboarding": "^8.6.10",
        "@storybook/blocks": "^8.6.10",
        "@storybook/react": "^8.6.10",
        "@storybook/react-vite": "^8.6.10",
        "@storybook/test": "^8.6.10",
        "@types/react": "^19.0.12",
        "@types/react-dom": "^19.0.4",
        "eslint": "^9.23.0",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-storybook": "^0.12.0",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.0",
        "prettier": "^3.5.3",
        "react": "^19.0.0",
        "storybook": "^8.6.10",
        "styled-components": "^6.1.16",
        "tsup": "^8.4.0",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.28.0"
    },
    "peerDependencies": {
        "styled-components": "^6.1.16"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx,css,md,json,mdx}": "prettier --write",
        "src/**/*.ts*": "eslint"
    },
    "eslintConfig": {
        "extends": [
            "plugin:storybook/recommended"
        ]
    }
}
